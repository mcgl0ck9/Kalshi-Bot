{
  "name": "kalshi-edge-detector",
  "version": "2.0.0",
  "description": "Multi-signal edge detection for Kalshi prediction markets",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsc",
    "test": "vitest",
    "run-now": "tsx src/index.ts --run-now",
    "bot": "tsx src/index.ts --bot",
    "scan": "tsx src/index.ts --scan",
    "v4": "node dist/v4.js",
    "v4:scan": "node dist/v4.js --scan",
    "v4:dev": "tsx watch src/v4.ts",
    "daemon:start": "./scripts/start-daemon.sh start",
    "daemon:stop": "./scripts/start-daemon.sh stop",
    "daemon:restart": "./scripts/start-daemon.sh restart",
    "daemon:status": "./scripts/start-daemon.sh status",
    "daemon:logs": "./scripts/start-daemon.sh logs",
    "daemon:monitor": "./scripts/start-daemon.sh monitor",
    "daemon:autostart": "./scripts/start-daemon.sh autostart"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcgl0ck9/Kalshi-Bot.git"
  },
  "keywords": [
    "kalshi",
    "polymarket",
    "prediction-markets",
    "edge-detection",
    "trading"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mcgl0ck9/Kalshi-Bot/issues"
  },
  "homepage": "https://github.com/mcgl0ck9/Kalshi-Bot#readme",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@alango/dr-manhattan": "^0.1.4",
    "chalk": "^5.6.2",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "natural": "^8.1.0",
    "node-fetch": "^3.3.2",
    "rss-parser": "^3.13.0",
    "sentiment": "^5.0.2",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/natural": "^5.1.5",
    "@types/node": "^25.0.3",
    "@types/sentiment": "^5.0.4",
    "@types/ws": "^8.18.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16"
  }
}
